<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <ErrorOnDuplicatePublishOutputFiles>false</ErrorOnDuplicatePublishOutputFiles>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>Xzy.SK.Domain.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Repositories\CRM\**" />
    <Compile Remove="Repositories\Mng\**" />
    <Compile Remove="Repositories\PMP\**" />
    <Compile Remove="Repositories\WF\**" />
    <EmbeddedResource Remove="Repositories\CRM\**" />
    <EmbeddedResource Remove="Repositories\Mng\**" />
    <EmbeddedResource Remove="Repositories\PMP\**" />
    <EmbeddedResource Remove="Repositories\WF\**" />
    <None Remove="Repositories\CRM\**" />
    <None Remove="Repositories\Mng\**" />
    <None Remove="Repositories\PMP\**" />
    <None Remove="Repositories\WF\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="8.1.0" />
    <PackageReference Include="FluentValidation.AspNetCore" Version="10.4.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="8.0.0" />
    <PackageReference Include="Microsoft.KernelMemory.Core" Version="0.26.240115.4" />
    <PackageReference Include="Microsoft.SemanticKernel" Version="1.0.1" />
    <PackageReference Include="Microsoft.SemanticKernel.Core" Version="1.0.1" />
    <PackageReference Include="Microsoft.SemanticKernel.Planners.Handlebars" Version="1.0.1-preview" />
    <PackageReference Include="Microsoft.SemanticKernel.Planners.OpenAI" Version="1.0.1-preview" />
    <PackageReference Include="Microsoft.SemanticKernel.Plugins.Core" Version="1.0.1-alpha" />
    <PackageReference Include="Microsoft.SemanticKernel.Plugins.Memory" Version="1.0.1-alpha" />
    <PackageReference Include="NPOI" Version="2.6.2" />
    <PackageReference Include="Polly" Version="8.0.0" />
    <PackageReference Include="RestSharp" Version="110.2.0" />
    <PackageReference Include="SqlSugarCore" Version="5.1.4.98" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Domain\Enum\" />
    <Folder Include="Domain\Interface\" />
    <Folder Include="Domain\Service\" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Xzy.SK.Domain.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
